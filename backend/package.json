{
  "name": "rtm-asset-explorer-backend",
  "version": "1.0.0",
  "description": "REST API server for Raptoreum Asset Explorer",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "start:sync": "node src/services/sync-daemon.js",
    "dev": "node --watch src/server.js",
    "test": "echo \"No tests yet\" && exit 0",
    "test:export": "node test-export.js",
    "test:integration": "node test-integration.js",
    "test:config": "node test-config.js",
    "test:validation": "node test-validation.js",
    "test:all": "npm run test:integration && npm run test:config && npm run test:validation",
    "fix:subassets": "node scripts/fix-subassets.js"
  },
  "keywords": [
    "raptoreum",
    "blockchain",
    "explorer",
    "api"
  ],
  "author": "Salty-Dragon",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^5.0.0",
    "form-data": "^4.0.5",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.0",
    "pdfkit": "^0.17.2",
    "redis": "^4.6.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "jpeg-exif": "npm:exif-reader@^2.0.3",
    "glob": "^13.0.0",
    "minimatch": "^10.2.1"
  }
}